<mvc:View xmlns:mvc="sap.ui.core.mvc" 
  xmlns="sap.m" 
  xmlns:core="sap.ui.core" 
  xmlns:chart="openui5.chart" 
  displayBlock="true">
  <App>
    <Page enableScrolling="false" 
      showHeader="false">
      <chart:Chart items="{/data}">
        <core:Item text="{y}"/>
      </chart:Chart>
    </Page>
  </App>
</mvc:View>
