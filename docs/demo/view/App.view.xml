<mvc:View xmlns:mvc="sap.ui.core.mvc" 
  xmlns="sap.m" 
  xmlns:core="sap.ui.core" 
  xmlns:chart="openui5.chart" 
  displayBlock="true">
  <App>
    <Page enableScrolling="false" 
      showHeader="false">
      <chart:Chart 
        padding="10 10 5 5">
      	<chart:series>
      		<chart:Series items="{/data}" >
		      	<chart:items>
		      		<core:Item key="{x}" text="{a}"/>
		      	</chart:items>
      		</chart:Series>
      		<chart:Series items="{/data}" >
		      	<chart:items>
		      		<core:Item key="{x}" text="{b}"/>
		      	</chart:items>
      		</chart:Series>
      	</chart:series>
        <chart:xAxes>
        	<chart:Axis size="20"/>
        </chart:xAxes>
        <chart:yAxes>
        	<chart:Axis size="30"/>
        </chart:yAxes>
      </chart:Chart>
    </Page>
  </App>
</mvc:View>
